<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StudentFormController">
   <children>
      <Label layoutX="225.0" layoutY="26.0" prefHeight="27.0" prefWidth="166.0" text="Student From" underline="true">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Button layoutX="269.0" layoutY="208.0" mnemonicParsing="false" onAction="#btnHelloWorldOnAction" style="-fx-background-color: #26de81; -fx-background-radius: 15px;" text="Enter" textFill="#ece7e7">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <TextField fx:id="txtStudentName" layoutX="158.0" layoutY="101.0" prefHeight="26.0" prefWidth="254.0" promptText="Enter Your Name">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label layoutX="62.0" layoutY="100.0" prefHeight="27.0" prefWidth="85.0" text="Student Name:" />
      <TextField fx:id="txtStudentAddress" layoutX="157.0" layoutY="175.0" prefHeight="26.0" prefWidth="254.0" promptText="Enter Your Address">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <Label layoutX="62.0" layoutY="174.0" prefHeight="27.0" prefWidth="94.0" text="Student Address:" />
      <Button layoutX="291.0" layoutY="127.0" mnemonicParsing="false" onAction="#btnPrintStudentName" text="Enter" textFill="#ff0000af">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="412.0" layoutY="175.0" mnemonicParsing="false" onAction="#btnPrintStudentAdress" text="Enter" textFill="#ff0000af">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="lblMassage" layoutX="137.0" layoutY="271.0" prefHeight="67.0" prefWidth="324.0" style="-fx-background-color: #d1d8e0; -fx-background-radius: 15px;" textAlignment="CENTER" textFill="#383f40">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="NONE" />
         </cursor>
      </Label>
      <Button layoutX="217.0" layoutY="127.0" mnemonicParsing="false" onAction="#btnAddStudentName" text="Add Name" textFill="#ff0000af">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="btnHide" layoutX="467.0" layoutY="271.0" mnemonicParsing="false" onAction="#btnHideOnAction" prefHeight="26.0" prefWidth="46.0" text="Hide" />
      <Button fx:id="btnShow" layoutX="467.0" layoutY="305.0" mnemonicParsing="false" onAction="#btnShowOnAction" text="Show" />
   </children>
</AnchorPane>
